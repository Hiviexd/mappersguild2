{
  "name": "mappersguild",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod": "set NODE_ENV=production&& node ./dist/app.js",
    "build": "webpack --mode production --config webpack.prod.config.js",
    "build-app": "tsc",
    "dev": "run-p -rl watch-app watch-src",
    "watch-app": "ts-node-dev --respawn --transpileOnly --no-notify app.ts",
    "watch-src": "webpack-dev-server --mode development --port 8080 --watch"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.17.0",
    "handlebars": "^4.5.3",
    "hbs": "^4.0.6",
    "mongoose": "^5.8.3",
    "morgan": "~1.9.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/mongoose": "^5.5.35",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "file-loader": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "src/*"
    ]
  }
}
