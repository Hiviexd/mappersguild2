{
  "name": "mappersguild",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod": "set NODE_ENV=production&& node ./dist/app.js",
    "build-src": "webpack --mode production --config webpack.prod.config.js",
    "build-app": "tsc --project tsconfig.prod.json",
    "dev": "run-p -rl watch-app watch-src",
    "watch-app": "ts-node-dev --respawn --transpileOnly --no-notify app.ts",
    "watch-src": "webpack-dev-server --watch",
    "lint": "eslint . --ext .ts,.js,.vue",
    "tsc": "tsc --noEmit true"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "handlebars": "^4.7.6",
    "hbs": "^4.1.1",
    "mongoose": "^5.9.21",
    "morgan": "~1.9.0",
    "vue": "^2.6.11",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.5.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/hbs": "^4.0.1",
    "@types/jquery": "^3.5.0",
    "@types/mongoose": "^5.7.29",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.4",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.2",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.6",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "src/*"
    ]
  }
}
